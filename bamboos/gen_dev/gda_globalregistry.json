{
    "globalregistry": {
        "config": {
            "input_dir": "rel/globalregistry",
            "target_dir": "rel/test_cluster"
        },
        "nodes": {
            "gr1": {
                "vm.args": {
                    "name": "gr1@",
                    "setcookie": "cookie1"
                },
                "sys.config": {
                    "db_nodes": [ "gr1_db1@", "gr1_db2@" ],
                    "grpcert_domain": "127.0.0.1"
                }
            },
            "gr2": {
                "vm.args": {
                    "name": "gr2@",
                    "setcookie": "cookie2"
                },
                "sys.config": {
                    "db_nodes": [ "gr2_db1@", "gr2_db2@" ],
                    "grpcert_domain": "127.0.0.1"
                }
            }
        }
    }
}
