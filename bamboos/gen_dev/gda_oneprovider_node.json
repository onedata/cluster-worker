{
    "oneprovider_node": {
        "config": {
            "input_dir": "rel/oneprovider_node",
            "target_dir": "rel/test_cluster"
        },
        "nodes": {
            "ccm1": {
                "vm.args": {
                    "name": "ccm1@",
                    "setcookie": "cookie"
                },
                "sys.config": {
                    "node_type": "ccm",
                    "ccm_nodes": [ "ccm1@" ],
                    "db_nodes": [ "127.0.0.1:49161" ],
                    "workers_to_trigger_init": 1
                }
            },
            "worker": {
                "vm.args": {
                    "name": "worker@",
                    "setcookie": "cookie"
                },
                "sys.config": {
                    "node_type": "worker",
                    "ccm_nodes": [ "ccm1@"],
                    "db_nodes": [ "127.0.0.1:49161" ]
                }
            }
        }
    }
}
